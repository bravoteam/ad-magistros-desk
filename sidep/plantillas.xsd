                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        P.Id_Personal, P.Ficha, ISNULL(P.RFC, '') AS RFC, P.CURP, P.Apellido_Paterno, P.Apellido_Materno, P.Nombre, P.Domicilio, P.numExt, P.numInt, ISNULL(CO.nombre, 
                         '') AS Colonia, MU.NOMBRE AS Municipio, ES.NOMBRE AS Estado, P.Telefono, P.Celular, C.nombre AS CT, C.CCT, NI.descripcion AS Nivel, T.descripcion AS Turno, 
                         ISNULL(M.materia, '') AS Materia, isnull(H.grado,'') as grado, ISNULL(H.Grupo, '') AS grupo,
                             (SELECT        COUNT(idMateria) AS Expr1
                               FROM            horarios
                               WHERE        (idPlantilla = PL.idPlantilla) AND (idMateria = H.idMateria) AND (Grupo = H.Grupo)) AS Horas, PL.HD, PL.HA, ISNULL(PU.descripcion, 'SIN PUESTO') 
                         AS Puesto, ISNULL(FU.Descripcion, 'NO ESPECIFICA') AS Funcion, isnull(M.horas,'') as carga,
                         isnull(M.agrupacion,'') as Agrupacion
FROM            Personal AS P INNER JOIN
                         plantillas AS PL ON P.Id_Personal = PL.idPersonal INNER JOIN
                         CT AS C ON C.IdCT = PL.idCCT INNER JOIN
                         turnos AS T ON T.idTurno = C.idTurno LEFT OUTER JOIN
                         horarios AS H ON H.idPlantilla = PL.idPlantilla LEFT OUTER JOIN
                         materias AS M ON M.id_materia = H.idMateria LEFT OUTER JOIN
                         colonia AS CO ON CO.id_colonia = P.Id_Colonia LEFT OUTER JOIN
                         municipio AS MU ON MU.ID_MUNICIPIO = P.Id_Localidad LEFT OUTER JOIN
                         estados AS ES ON ES.ID_ESTADO = MU.ID_ESTADO LEFT OUTER JOIN
                         puestos AS PU ON PU.idPuesto = PL.idPuesto LEFT OUTER JOIN
                         funciones AS FU ON FU.idFuncion = PL.idFuncion LEFT OUTER JOIN
                         niveles AS NI ON NI.idNivel = C.idNivel
WHERE        (C.IdCT = @idCT)
GROUP BY C.idNivel, PL.idPlantilla, P.Ficha, ISNULL(P.RFC, ''), P.CURP, P.Apellido_Paterno, P.Apellido_Materno, P.Nombre, P.Domicilio, P.numExt, P.numInt, CO.nombre, 
                         MU.NOMBRE, ES.NOMBRE, P.Telefono, P.Celular, C.nombre, C.CCT, NI.descripcion, T.descripcion, M.materia, PU.descripcion, FU.Descripcion, H.idMateria, PL.HD, 
                         PL.HA, P.Id_Personal, H.Grupo,H.grado,m.horas,m.agrupacion
ORDER BY C.idNivel, CT, P.Apellido_Paterno, P.Apellido_Materno, P.Nombre</CommandText>
