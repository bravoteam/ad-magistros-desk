                    <CommandText>SELECT        h.dia, h.horaInicio, h.horaFin, m.materia, m.horas AS carga, h.Grado, h.Grupo, ISNULL(m.agrupacion, '') AS AGRUPACION, UPPER(LTRIM(RTRIM(pe.Nombre)) 
                         + ' ' + LTRIM(RTRIM(pe.Apellido_Paterno)) + ' ' + LTRIM(RTRIM(pe.Apellido_Materno))) AS nombre_completo, CT.nombre AS escuela, t.descripcion AS turno, 
                         n.descripcion AS nivel
FROM            horarios AS h INNER JOIN
                         plantillas AS p ON p.idPlantilla = h.idPlantilla INNER JOIN
                         CT ON CT.IdCT = p.idCCT INNER JOIN
                         Personal AS pe ON pe.Id_Personal = p.idPersonal INNER JOIN
                         materias AS m ON m.id_materia = h.idMateria INNER JOIN
                         turnos AS t ON t.idTurno = CT.idTurno INNER JOIN
                         niveles AS n ON n.idNivel = CT.idNivel
WHERE        (CT.IdCT = @idCT)</CommandText>
